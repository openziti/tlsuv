{
  "name": "tlsuv",
  "version-semver": "1.0.0",
  "dependencies": [
    "libuv"
  ],
  "default-features": [ "tls", "http" ],
  "features": {
    "tls": {
      "description": "default TLS implementation",
      "dependencies": [
        {
          "name": "tlsuv",
          "platform": "osx | ios",
          "default-features": false,
          "features": [ "applesec" ]
        },
        {
          "name": "tlsuv",
          "platform": "!(osx | ios)",
          "default-features": false,
          "features": [ "openssl" ]
        }
      ]
    },
    "openssl": {
      "description": "use OpenSSL",
      "dependencies": [ "openssl" ]
    },
    "mbedtls": {
      "description": "use mbedTLS",
      "dependencies": [ "mbedtls" ]
    },
    "http": {
      "description": "adds HTTP and websocket support",
      "dependencies": [
        "zlib",
        "llhttp"
      ]
    },
    "applesec": {
      "description": "Apple SSL support",
      "supports": "osx | ios",
      "dependencies": [
        "b64"
      ]
    },
    "test": {
      "description": "Dependencies for testing",
      "dependencies": [
        {
          "name": "catch2",
          "version>=": "2.13.9#1"
        },
        {
          "name": "parson",
          "$comment": "test response parsing/validations"
        }
      ]
    },
    "samples": {
      "description": "Dependencies for samples",
      "dependencies": [
        {
          "name": "getopt-win32",
          "platform": "windows & !mingw"
        }
      ]
    },
    "all-deps": {
      "description": "All dependencies",
      "dependencies": [
        {
          "name": "tlsuv",
          "features": [ "test", "samples", "mbedtls" ]
        }
      ]
    }
  },
  "overrides": [
  ],
  "builtin-baseline": "3508985146f1b1d248c67ead13f8f54be5b4f5da"
}
